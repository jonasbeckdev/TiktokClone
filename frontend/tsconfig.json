{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "modules/*": ["src/modules/*"],
            "components": ["src/components"],
            "hooks": ["src/hooks"],
            "styles": ["assets/styles"],
            "screens": ["src/screens"],
            "navigation": ["src/navigation"],
            "reduxs/*": ["src/redux/*"]
        }
    },
    "extends": "expo/tsconfig.base",
    "exclude": [
        "node_modules",
        "dist"
    ]
}